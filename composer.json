{
    "name": "phauthenticate/authentication-yii",
    "require": {
        "yiisoft/yii2": "^2.0.15.1",
        "cakephp/authentication": "2.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "phpunit/dbunit": "^4.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Kr√§mer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phauthentic\\Authentication\\": [
                "src/",
                "vendor/cakephp/authentication/src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phauthentic\\Authentication\\Test\\": [
                "tests/",
                "vendor/cakephp/authentication/tests/"
            ]
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:burzum/authentication.git"
        }
    ],
    "minimum-stability": "dev"
}
